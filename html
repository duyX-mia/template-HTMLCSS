    <meta charset="UTF-8">
    <title>Page Title</title>
    <!-- Viewport size for responsive design -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Link to an external stylesheet -->
    <link rel="stylesheet" href="style.css">
  </head>
  <body>
    <!-- Header section, typically contains site logo and main navigation -->
    <header>
      <nav>
        <ul>
          <li><a href="#">Home</a></li>
          <li><a href="#">About</a></li>
          <li><a href="#">Contact</a></li>
        </ul>
      </nav>
    </header>
    <!-- Main content area, typically contains several sections or articles -->
    <main>
      <!-- Section with a heading and a paragraph of text -->
      <section>
        <h1>Section Heading</h1>
        <p>This is a paragraph of text.</p>
      </section>
    </main>
    <!-- Footer section, typically contains copyright information and social media links -->
